CREATE TABLE AIRLINE (
    AIRLINEID VARCHAR(3) NOT NULL,
    AL_NAME VARCHAR2(50),
    THREE_DIGIT_CODE VARCHAR(3),
    PRIMARY KEY(AIRLINEID)
);
